                                <h5 class="card-title">@item.ProjectName Project</h5>
                                <p class="text-black">@item.AboutProject </p>
                            </div>
                            <a href="@Url.Action("SingleProject","projects", new {id=item.IdProject})" class="btn btn-primary" style="margin-bottom:0px">Show Details</a>

                        </div>}
            }

        </div>
    </div>
</section>

